{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "indent": [
      true,
      "spaces",
      2
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-console": false,
    "no-empty": false,
    "interface-name": false
  },
  "rulesDirectory": []
}
